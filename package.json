{
  "name": "holiday-planner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma migrate reset -f && prisma generate && concurrently \"next dev\" \"local-ssl-proxy --key localhost-key.pem --cert localhost.pem --source 3001 --target 3000\"",
    "build": "prisma migrate reset -f && prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "resetdb": "npx prisma db push --force-reset && npx prisma db seed",
    "generateseeddata": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/generateSeedData.ts"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.0.0",
    "@headlessui/react": "^2.0.0",
    "@heroicons/react": "^2.0.18",
    "@prisma/client": "^6.0.0",
    "@reduxjs/toolkit": "^2.0.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tremor/react": "^3.8.2",
    "@types/react": "18.3.21",
    "@types/react-dom": "18.3.7",
    "autoprefixer": "10.4.21",
    "class-transformer": "^0.5.1",
    "decompress": "^4.2.1",
    "dotenv": "^16.3.1",
    "eslint": "8.57.1",
    "eslint-config-next": "^14.0.0",
    "fast-xml-parser": "^5.0.0",
    "google-maps-react-markers": "^2.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.3",
    "next": "^14.0.0",
    "postcss": "8.5.6",
    "prisma": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.0",
    "react-virtuoso": "^4.6.1",
    "recoil": "^0.7.7",
    "reflect-metadata": "^0.2.0",
    "sharp": "^0.34.0",
    "swr": "^2.2.4",
    "tailwindcss": "3.4.17"
  },
  "devDependencies": {
    "@types/decompress": "^4.2.5",
    "@types/google.maps": "^3.54.3",
    "@types/lodash": "^4.14.199",
    "@types/luxon": "^3.3.2",
    "@types/node": "^22.0.0",
    "concurrently": "^9.0.0",
    "local-ssl-proxy": "^2.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2"
  }
}
