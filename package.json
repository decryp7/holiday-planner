{
  "name": "holiday-planner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma migrate reset -f && prisma generate && concurrently \"next dev\" \"local-ssl-proxy --key localhost-key.pem --cert localhost.pem --source 3001 --target 3000\"",
    "build": "prisma migrate reset -f && prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "resetdb": "npx prisma db push --force-reset && npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.38",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@prisma/client": "^5.3.1",
    "@react-google-maps/api": "^2.19.2",
    "@tailwindcss/forms": "^0.5.6",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "eslint": "8.49.0",
    "eslint-config-next": "^13.5.2",
    "next": "^13.5.2",
    "postcss": "8.4.29",
    "prisma": "^5.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.6.5",
    "concurrently": "^8.2.1",
    "local-ssl-proxy": "^2.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
